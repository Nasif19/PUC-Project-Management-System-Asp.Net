                            <ul class="nav nav-main">

                                <!-- Dashboard -->
                                <li class="nav-active">
                                    <a href="@Url.Action("Index","ProjectGroup")">
                                        <i class="fas fa-users-cog"></i>
                                        <span>Group Management</span>
                                    </a>
                                </li>

                                <!--Groups-->
                                <li class="nav-active">
                                    <a href="@Url.Action("Index","ProjectList")">
                                        <i class="fas fa-swatchbook"></i>
                                        <span>Project Management</span>
                                    </a>

                                </li>
                                <!--Request-->
                                <li class="nav-active">
                                    <a href="@Url.Action("Index","ProjectRequest")">
                                        <i class="fas fa-layer-group"></i>
                                        <span>Project Request</span>
                                    </a>

                                </li>
                                <!--Report-->
                                <li class="nav-active">
                                    <a href="@Url.Action("Index","Report")">
                                        <i class="fas fa-layer-group"></i>
                                        <span>Report</span>
                                    </a>

                                </li>
                            </ul>
                        </nav>

                    </div>
                </div>
